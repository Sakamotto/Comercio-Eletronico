<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS, and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<div class="row">
    <div class="col-md-4">
        <h2>Data Início</h2>
        <input type="text" id="dataInicio">
    </div>
    <div class="col-md-4">
        <h2>Data Término</h2>
        <input type="text" id="dataTermino">
    </div>
    <div class="col-md-4">
        <p><a class="btn btn-default" onclick="GetAll()">Buscar &raquo;</a></p>
    </div>
</div>

<script>
    var dataI = null;
    var dataT = null;

    function GetAll() {
        console.log("this.dataInicio: ", dataI);
        $.get("/api/Hotel/GetHoteisPorData/", { dataInicio: dataI, dataTermino: dataT, guests: 2 }, function (data) {
            console.log("Retorno: ", data);
        })
    }

    $("#dataInicio").datepicker({
        onSelect: function (dataText, inst) {
            dataI = new Date(dataText.replace(/\//g, "-")).toISOString();
            console.log("Data início: ", dataText, dataI);
        }
    });
    $("#dataTermino").datepicker({
        onSelect: function (dataText, inst) {
            dataT = new Date(dataText.replace(/\//g, "-")).toISOString()
            console.log("Data Término: ", dataText, dataT);
        }
    });
</script>
